# Ignore directories generated by Composer
/vendor/
/docroot/sites/all/drush/
/docroot/sites/all/modules/contrib/
/docroot/sites/all/themes/contrib/
/docroot/sites/all/libraries/

# Ignore Drupal 7 core files.
/docroot/*
!/docroot/*/
/docroot/includes/
/docroot/misc/
/docroot/modules/
/docroot/profiles/README.txt
/docroot/profiles/minimal/
/docroot/profiles/standard/
/docroot/profiles/testing/
/docroot/scripts/
/docroot/sites/README.txt
/docroot/sites/all/*/README.txt
/docroot/sites/example.sites.php
/docroot/themes/

# Ignore configuration files that may contain sensitive information.
/docroot/sites/*/*.settings.php
/docroot/sites/*/settings*.php

# Ignore paths that contain user-generated content.
/docroot/sites/*/files
/docroot/sites/*/private

# Ignore files generated by PhpStorm
/.idea/

/z-local/
